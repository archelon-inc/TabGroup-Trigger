{
  "manifest_version": 3,
  "name": "TabGroup Trigger",
  "version": "1.0.0",
  "description": "Switch Chrome Tab Groups via URL invocation",
  "permissions": [
    "tabs",
    "tabGroups",
    "webNavigation",
    "history"
  ],
  "host_permissions": [
    "https://extension.tabgroup-trigger/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
